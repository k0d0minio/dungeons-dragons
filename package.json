{
  "name": "tailwind-plus-transmit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && prisma migrate deploy && next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "db:deploy": "prisma migrate deploy",
    "db:start": "docker compose up postgres -d",
    "db:stop": "docker compose down postgres"
  },
  "browserslist": "defaults, not ie <= 11",
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "@supabase/supabase-js": "^2.57.4",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/typography": "^0.5.16",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "jsonwebtoken": "^9.0.2",
    "next": "^15",
    "react": "^19",
    "react-aria": "^3.42.0",
    "react-dom": "^19",
    "react-hook-form": "^7.63.0",
    "react-stately": "^3.40.0",
    "rss-to-json": "^2.1.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "valibot": "^1.1.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "eslint": "^9.32.0",
    "eslint-config-next": "^15",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prisma": "^6.16.2",
    "sharp": "0.34.3"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
